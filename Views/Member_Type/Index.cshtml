@model IEnumerable<DatabaseMgmt.Views.Member_Type>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.maxBooks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.limitDuration)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.maxBooks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.limitDuration)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.memberTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.memberTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.memberTypeID })
        </td>
    </tr>
}

</table>
