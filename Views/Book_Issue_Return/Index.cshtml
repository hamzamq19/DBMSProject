            @Html.DisplayFor(modelItem => item.BookCopy.bookCopyID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Book.bookName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FinePolicy.finePolicyID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Librarian.librarianName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.memberName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.issueID }) |
            @Html.ActionLink("Details", "Details", new { id=item.issueID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.issueID })
        </td>
    </tr>
}

</table>
