        <th>
            @Html.DisplayNameFor(model => model.Book.bookName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FinePolicy.finePolicyID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Librarian.librarianName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member.memberName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.issueDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dueDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.returnDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.payDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
