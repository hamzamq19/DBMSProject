@model IEnumerable<DatabaseMgmt.Views.Member>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.memberName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.memberAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.memberEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.memberContact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member_Type.memberTypeID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.memberName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.memberAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.memberEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.memberContact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member_Type.memberTypeID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.memberID }) |
            @Html.ActionLink("Details", "Details", new { id=item.memberID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.memberID })
        </td>
    </tr>
}

</table>
